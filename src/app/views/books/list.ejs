<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Listagem de livros</title>
  </head>
  <body>
    <h1>Listagem de livros</h1>

    <table>
      <thead>
        <th>Id</th>
        <th>title</th>
      </thead>

      <tbody>
        <% books.forEach(book => { %>
        <tr>
          <td><%=book.id %></td>
          <td><%=book.titulo %></td>
        </tr>

        <% }) %>
      </tbody>
    </table>
  </body>
</html>
